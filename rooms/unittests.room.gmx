//if-syntax is used to categorize tests
var unittest = true;

///array_init
if(unittest)
{
    var array_1d = array_init(2);
    assert(array_1d[0] == 0);
    assert(array_1d[1] == 0);
    
    var array_2d = array_init(2, 2);
    assert(array_2d[0,0] == 0);
    assert(array_2d[0,1] == 0);
    assert(array_2d[1,0] == 0);
    assert(array_2d[1,1] == 0);
}

///array_create
if(unittest)
{
    var array = array_create(0, 1, "two", 3, 4);
    assert(array[3] == 3);
    assert(array[2] == "two");
}

///array_equal
if(unittest)
{
    var array1 = array_create(1,2,3,4);
    var array2 = array_create(1,2,3,4);
    assert(array_equal(array1, array2));
    
    array1[0] = "number";
    assert(!array_equal(array1, array2));
    
    array1 = array_init(4, 2);
    array2 = array_init(4, 2);
    assert(array_equal(array1, array2));
    
    array1[2,4] = 42;
    assert(!array_equal(array1, array2));
}

///array_at
if(unittest)
{
    var array_1d = 0;
    array_1d[2] = 42;
    assert(array_at(array_1d, 0) == 0);
    assert(array_at(array_1d, 2) == 42);
    
    var array_2d = 0;
    array_2d[4, 2] = 3;
    assert(array_at(array_2d, 4, 2) == 3);
}

///array_append
if(unittest)
{
    
}

///array_copy
if(unittest)
{
    //copy 1D arrays
    var array_1d = 0;
        array_1d[0] = 42;
    
    var new_array_1d = array_copy(array_1d);
        new_array_1d[0] = 3;
    
    assert(new_array_1d[0] != array_1d[0]);
    assert(array_1d[0] == 42);
    
    //copy 2D arrays
    var array_2d = 0;
        array_2d[4, 2] = 6;
    var new_array_2d = array_copy(array_2d);
        new_array_2d[4, 2] = 5;
    
    assert(array_at(array_2d, 4, 2) == 6);
    assert(array_at(new_array_2d, 4, 2) != 6);
    assert(array_at(new_array_2d, 4, 2) == 5);
}

///array_sub
if(unittest)
{
    var array_2d = array_init(3,2);
    array_2d[0,0] = "hello"; array_2d[0,1] = "world"; array_2d[0,2] = "!!!"; 
    array_2d[1,0] = 1; array_2d[1,1] = 2; array_2d[1,2] = 3; 
    
    //assert(array_equal(array_sub(array_2d, 0), array_create("hello", "world", "!!!")));
    //assert(array_equal(array_sub(array_2d, 1), array_create(1, 2, 3)));
}

///array_split
if(unittest)
{
    var splits = array_split("hello-world-again.", "-");
    var test = 0;
    test[0] = "hello";
    test[1] = "world";
    test[2] = "again.";

    assert(splits[0] == test[0]);
    assert(splits[1] == test[1]);
    assert(splits[2] == test[2]);
}

///array_reverse
if(unittest)
{
    var array_1d = array_create(1,2,3,4);
    //array_reversed modifies original array.
    var array_1d_reversed = array_reverse(array_1d);
    assert(array_equal(array_1d_reversed, array_create(4,3,2,1)));
}

///array_find
if(unittest)
{
    //find first value
    var array_1d = array_create(1,2,3,4);
    assert(array_find(array_1d, 3) == 2);
    assert(array_find(array_1d, 16) == -1);
    
    //find 2nd value
    array_1d = array_create("number", "", "", "number");
    assert(array_find(array_1d, "number", 2) != 0);
    assert(array_find(array_1d, "number", 2) == 3);
}

///array_count
if(unittest)
{
    var array_1d = array_create(1,4,1,1,"tree");
    assert(array_count(array_1d, 1) == 3);
    assert(array_count(array_1d, 4) == 1);
    assert(array_count(array_1d, "car") == 0);
}

//array_exists
if(unittest)
{
    var array_1d = array_create(1,2,3,4,5);
    assert(array_exists(array_1d, 1) == true);
    assert(array_exists(array_1d, 6) == false);
    assert(array_exists(array_1d, "tree") == false);
}

///array_expand
if(unittest)
{
    var a1 = array_create("abs","dps","fps");
    var a2 = array_create(3, 4, 5);
    var a3 = array_create(a1, a2);
    var a4 = array_create("tree", a3);
    var a5 = array_create("ball", a4, "house");
 
    //nested arrays
    var expanded_array = array_expand(a3);
    assert(array_equal(expanded_array, array_create("abs","dps","fps",3,4,5)));
    
    //multiple layers of arrays nested
    expanded_array = array_expand(a5);
    assert(array_equal(expanded_array, array_create("ball","tree","abs","dps","fps",3,4,5,"house")));
    
    log("asdasd");
    //level limit
    expanded_array = array_expand(a5);
    
    //no expansion
    var array = array_create("test");
    assert(array_equal(array_expand(array), array));
}

///array_length
if(unittest)
{
    var array_1d = array_create(42,134,"house");
    assert(array_length(array_1d) == 3);
    
    var array_2d = 0;
    array_2d[0,3] = 0;
    array_2d[1,2] = 0;
    assert(array_length(array_2d)    == 4);
    assert(array_length(array_2d, 0) == 4);
    assert(array_length(array_2d, 1) == 3);
}

///array_slice
if(unittest)
{
    var array_1d = array_create(0,1,2,3,4,5,6,7);
    
    assert(array_equal(array_slice(array_1d, 0, 3), array_create(0,1,2)));
    assert(array_equal(array_slice(array_1d, 5, 8), array_create(5,6,7)));
}

///array_insert
if(unittest)
{
    var array_1d = array_create(1,2,3,5);
    array_1d = array_insert(array_1d, 3, 4);
    assert(array_equal(array_1d, array_create(1,2,3,4,5)));
    
    array_1d = array_create("house", "ball", "car");
    array_insert(array_1d, 0, "number");
    assert(array_equal(array_1d, array_create("number","house","ball","car")));
}

///array_string
if(unittest)
{
    var str = "12345";
    var array = array_string(str);
    assert(string_length(str) == array_length(array));
    for(var n = 0; n &lt; string_length(str); ++n)
    {
        assert(array[n] == string_char_at(str, n + 1));
    }
}

///array_sort
if(unittest)
{
    var array = array_create(2,5,4,1,3);
    //ascending
    var sorted = array_sort(array);
    assert(array_equal(sorted, array_create(1,2,3,4,5)));
    
    array = array_create(2,5,4,1,3);
    //descending
    sorted = array_sort(array, false);
    assert(array_equal(sorted, array_create(5,4,3,2,1)));
    
    array = array_create("banana", "apple", "asteroid");
    assert(array_equal(array_sort(array), array_create("apple", "asteroid", "banana")));
}

///array_is_1d
if(unittest)
{
    var array = array_init(4);
    assert(array_is_1d(array));
    
    array = array_init(3, 2);
    assert(!array_is_1d(array));
    
    array = array_init(1, 5);
    assert(array_is_1d(array));
}

///array_replace
if(unittest)
{
    var array = array_create("banana", "pineapple", "lumberjack");
    array = array_replace(array, 1, 42);
    assert(array_equal(array, array_create("banana", 42, "lumberjack")));
    
    array = array_create("one", "two", "three");
    array_replace(array, 2, 3);
    assert(array_equal(array, array_create("one", "two", 3)));
}

///real_within
if(unittest)
{
    assert( real_within(7,    5, 10));
    assert( real_within(8.65, 5, 10));
    assert( real_within(5,    5, 10));
    assert( real_within(10,   5, 10));
    assert(!real_within(15,   5, 10));
    assert(!real_within(3,    5, 10));
